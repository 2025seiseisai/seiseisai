{
    "name": "@seiseisai/ui",
    "version": "0.1.0",
    "private": true,
    "exports": {
        "./components/*": {
            "import": "./src/components/*.tsx"
        },
        "./lib/*": {
            "import": "./src/lib/*.ts"
        },
        "./hooks/*": {
            "import": "./src/hooks/*.ts"
        },
        "./styles/*": {
            "import": "./src/styles/*"
        }
    },
    "scripts": {
        "lint": "eslint . && cd ../../ && prettier --cache --check ./packages/ui",
        "format": "eslint . --fix && cd ../../ && prettier --cache --write ./packages/ui",
        "check-types": "tsc --noEmit"
    }
}
