{
    "name": "@seiseisai/ui",
    "version": "0.1.0",
    "private": true,
    "exports": {
        "./components/*": {
            "import": "./src/components/*.tsx"
        },
        "./lib/*": {
            "import": "./src/lib/*.ts"
        },
        "./hooks/*": {
            "import": "./src/hooks/*.ts"
        }
    },
    "scripts": {
        "lint": "eslint . && prettier --check .",
        "format": "eslint . --fix && prettier --write .",
        "check-types": "tsc --noEmit"
    }
}
