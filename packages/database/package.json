{
    "name": "@seiseisai/database",
    "version": "0.1.0",
    "private": true,
    "exports": {
        ".": {
            "import": "./src/index.ts"
        },
        "./models": {
            "import": "./src/models.ts"
        },
        "./enums": {
            "import": "./src/enums.ts"
        }
    },
    "scripts": {
        "lint": "cd ../../ && eslint ./packages/database --cache --cache-location ./node_modules/.cache/eslint/.eslintcache && prettier --cache --check ./packages/database",
        "format": "cd ../../ && eslint ./packages/database --cache --cache-location ./node_modules/.cache/eslint/.eslintcache --fix && prettier --cache --write ./packages/database",
        "check-types": "cd ../../ && prisma generate --schema=./packages/database/prisma/schema.prisma && cd ./packages/database && tsc --noEmit",
        "dev": "bun --env-file=../../.env ./scripts/insert-superadmin.ts",
        "build": "bun --env-file=../../.env ./scripts/insert-superadmin.ts"
    }
}
