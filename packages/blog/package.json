{
    "name": "@seiseisai/blog",
    "version": "0.1.0",
    "private": true,
    "exports": {
        "./data": "./src/data.ts"
    },
    "scripts": {
        "lint": "cd ../../ && eslint ./packages/blog --cache --cache-location ./node_modules/.cache/eslint/.eslintcache && prettier --cache --check ./packages/blog",
        "format": "cd ../../ && eslint ./packages/blog --cache --cache-location ./node_modules/.cache/eslint/.eslintcache --fix && prettier --cache --write ./packages/blog",
        "check-types": "tsc --noEmit",
        "generate-blog-data": "bun ./scripts/generate-blog-data.ts"
    },
    "dependencies": {
        "@seiseisai/settings": "workspace:*"
    }
}
