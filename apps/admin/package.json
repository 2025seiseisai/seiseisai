{
    "name": "@seiseisai/admin",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "opener http://localhost:3001/ && bun --env-file=../../.env next dev --turbopack --port 3001",
        "build": "bun --env-file=../../.env next build --turbopack",
        "start": "next start --port 3001",
        "lint": "cd ../../ && eslint ./apps/admin --cache --cache-location ./node_modules/.cache/eslint/.eslintcache && stylelint \"./apps/admin/src/**/*.(scss|less|css)\" --cache --cache-location ./node_modules/.cache/stylelint/.stylelintcache && prettier --cache --check ./apps/admin",
        "format": "cd ../../ && eslint ./apps/admin --fix --cache --cache-location ./node_modules/.cache/eslint/.eslintcache && stylelint \"./apps/admin/src/**/*.(scss|less|css)\" --cache --cache-location ./node_modules/.cache/stylelint/.stylelintcache --fix && prettier --cache --write ./apps/admin",
        "check-types": "next typegen && tsc --noEmit"
    },
    "dependencies": {
        "@seiseisai/database": "workspace:*",
        "@seiseisai/ui": "workspace:*",
        "@seiseisai/turnstile": "workspace:*",
        "@seiseisai/date": "workspace:*"
    }
}
