{
    "name": "seiseisai-admin",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "deploy": "bun ./scripts/deploy.mjs",
        "dev": "bun --env-file=.env ./scripts/prepare.mjs && bun install && prisma generate && opener http://localhost:3001 && next dev --turbopack --port 3001",
        "build": "prisma generate && next build",
        "start": "next start --port 3001",
        "lint": "next lint && stylelint \"src/**/*.(scss|less|css)\" && prettier --check .",
        "format": "next lint --fix && stylelint \"src/**/*.(scss|less|css)\" --fix && prettier --write .",
        "clean": "modclean --run",
        "type-check": "prisma generate && tsc --noEmit",
        "prisma:generate": "prisma generate",
        "prisma:db-pull": "prisma db pull",
        "prisma:db-push": "prisma db push",
        "prisma:studio": "prisma studio"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.1.1",
        "@next/third-parties": "^15.3.4",
        "@prisma/client": "^6.11.1",
